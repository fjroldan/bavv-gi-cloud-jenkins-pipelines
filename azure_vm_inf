pipeline {
	// Define the agent
    agent { label 'production_agent' }
    // Define the staging
	stages {
        stage ("provisioning") {
            sh "echo '[INFO]: Clonando repositorio de terraform...'"
            sh "git clone https://github.com/fjroldan/bavv-gi-cloud-terraform-azure-vm.git"
            sh "ls"
        }
		stage ("terraform init") {
			steps {
				sh ('terraform init')
			}
		}
	}
}